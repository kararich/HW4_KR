<h1><%= @place["name"] %></h1>

<h2>My Entries</h2>
<% for entry in @entries %>
  <h3><%= entry["title"] %></h3>
  <h4><%= entry["occurred_on"] %></h4>
  <p><%= entry["description"] %></p>
<% end %>

<p>
  <a href="/entries/new?place_id=<%= @place["id"] %>"><button class="btn btn-primary">New Entry</button></a>
</p>

<p>
  <a href="/places"><button class="btn btn-primary">Back to all Places</button></a>
</p>